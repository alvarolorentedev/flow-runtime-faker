{
  "name": "flow-runtime-generators",
  "version": "0.0.1",
  "description": "object generation based on flow description",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watchAll test/unit",
    "test:int": "jest test/integration",
    "test:cov": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "push": "yarn test && git push",
    "deploy:major": "yarn version --major",
    "deploy:minor": "yarn version --minor",
    "deploy:patch": "yarn version --patch",
    "deploy:push": "git push && git push --tags",
    "preversion": "yarn test"
  },
  "keywords": [],
  "repository": "git@github.com:kanekotic/flow-runtime-generators.git",
  "author": "kanekotic <alvarojosepl@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "flow-runtime": "0.17.0"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "faker": "4.1.0",
    "jest": "23.6.0"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*-test.js"
    ],
    "transform": {
      "^.+\\.jsx|.js?$": "babel-jest"
    }
  }
}
